{
  "name": "prettier-plugin-unused-imports-configurable",
  "version": "1.14.0",
  "description": "Prettier plugin configurable to remove unused imports from JavaScript and TypeScript files.",
  "main": "dist/index.js",
  "author": "Lisbaldy de Jesus",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "format": "prettier --log-level debug --write src/**/*.tsx"
  },
  "dependencies": {
    "ts-morph": "24.0.0"
  },
  "peerDependencies": {
    "prettier": "3.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@vitest/coverage-v8": "2.1.8",
    "rollup": "4.30.1",
    "rollup-plugin-terser": "7.0.2",
    "vitest": "2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lisba/prettier-plugin-unused-imports-configurable.git"
  },
  "bugs": {
    "url": "https://github.com/Lisba/prettier-plugin-unused-imports-configurable/issues"
  },
  "homepage": "https://github.com/Lisba/prettier-plugin-unused-imports-configurable#readme",
  "keywords": [
    "prettier",
    "plugin",
    "unused imports",
    "typescript",
    "javascript",
    "monorepo",
    "configurable"
  ]
}
